<snippet>
	<content><![CDATA[
add_action( 'genesis_meta', '${1:prefix}_viewport_meta_tag' );
/**
 * Outputs the viewport meta tag to the document head
 *
 * @return void
 */
function ${1:prefix}_viewport_meta_tag() {
	echo '<meta name="viewport" content="width=device-width, initial-scale=1.0"/>';
}
]]></content>
	<tabTrigger>WPST</tabTrigger>
	
	<description>viewport for RWD</description>
</snippet>