<snippet>
	<content><![CDATA[
add_filter( 'genesis_search_button_text', '${1:prefix}_search_button_text' );
/**
 * Returns the customized search button text
 *
 * @param  string \$text the current search text
 * @return string the customized text
 */
function ${1:prefix}_search_button_text( \$text ) {
	return esc_attr( '${2:Go}' );
}
]]></content>
	<tabTrigger>WPST</tabTrigger>
	
	<description>Customize Search Button Text</description>
</snippet>