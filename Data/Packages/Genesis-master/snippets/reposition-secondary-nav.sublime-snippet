<snippet>
	<content><![CDATA[
# Put the secondary navigation before the header
remove_action( 'genesis_after_header', 'genesis_do_subnav' );
add_action( 'genesis_before_header', 'genesis_do_subnav' );
]]></content>
	<tabTrigger>WPST</tabTrigger>
	
	<description>Reposition Subnav</description>
</snippet>